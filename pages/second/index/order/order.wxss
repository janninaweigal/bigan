@import "../../../template/tabbar/tabbar.wxss";
.order-bottom{
  width:100%;
  background-color:#fff;
  border-top:1px solid #000;
  box-sizing:border-box;
  padding:10px 10px;
  text-align:right;
}
.order-bottom .order-bottom-btn{
  display:inline-block;
  margin-left:10px;
  background-color:#D7D7D7;
  padding:5px 15px;
  box-sizing:border-box;
  font-size:12px;
  border-radius:15px;
}